{
  "expo": {
    "name": "MindHaven",
    "slug": "mindhaven",
    "owner": "zanderdev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "entryPoint": "./App.tsx",
    "splash": {
      "image": "./assets/Background.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#1C1C1C",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We use your photo library to let you pick a profile picture.",
        "NSMicrophoneUsageDescription": "This app records short voice notes to convert speech to text.",
        "NSSpeechRecognitionUsageDescription": "Converts your voice to text."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1C1C1C"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "RECORD_AUDIO",
        "RECORD_AUDIO"
      ],
      "package": "com.zanderdev.MindHaven"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-video",
      "expo-asset",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "bfb31303-2675-4be7-98b3-a18ce11264dc"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/bfb31303-2675-4be7-98b3-a18ce11264dc"
    }
  }
}
